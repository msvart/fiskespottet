function myFunction() {
  var checkboxSjælland = document.getElementById('Sjælland');
  var checkboxJylland = document.getElementById('Jylland');
  var checkboxFyn = document.getElementById('Fyn');

  var checkBoxKyst = document.getElementById('kyst');
  var checkBoxSø = document.getElementById('Sø');
  var checkBoxPat = document.getElementById('Pat')
  var text = document.getElementById("text");

  if (checkBoxKyst.checked == true && checkBoxSjælland) {
    window.location.replace("http://www.w3schools.com");
  } else if (checkBoxKyst.checked == true && checkBoxJylland) {
    window.location.replace("http://www.w3schools.com");
  } else if (checkBoxKyst.checked == true && checkBoxFyn){
    window.location.replace("http://www.w3schools.com");
  } else if (checkBoxSø.checked == true && checkBoxSjælland) {
    window.location.replace("http://www.facebook.com");
  } else if (checkBoxSø.checked == true && checkBoxJylland) {
    window.location.replace("http://www.facebook.com");
  } else if (checkBoxSø.checked == true && checkBoxFyn) {
    window.location.replace("http://www.facebook.com");
  } else if (checkBoxPat.checked == true && checkBoxSjælland) {
    window.location.replace("http://google.dk");
  } else if (checkBoxPat.checked == true && checkBoxJylland) {
    window.location.replace("http://google.dk");
  } else if (checkBoxPat.checked == true && checkBoxFyn) {
    window.location.replace("http://google.dk");
  } else {
    console.log('Intet valgt!');
  }
};
